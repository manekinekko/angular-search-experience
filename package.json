{
  "name": "angular-search-experience",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "install:functions": "cd functions && npm install",
    "postinstall:functions": "cd ..",
    "start": "ng serve",
    "start:backend": "cd functions && node server.js",
    "start:backend:firebase": "cd functions && firebase serve --only functions",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "build:ssr": "npm run build:client-and-server-bundles && npm run compile:server",
    "build:ssr:hosting": "npm run build:client-and-server-bundles && gulp firebase:ssr",
    "serve:ssr": "node dist/server",
    "build:client-and-server-bundles": "ng build --prod && ng run angular-search-experience:server:production",
    "compile:server": "tsc -p server.tsconfig.json",
    "test": "ng test",
    "test:ci": "echo 'sorry, no tests yet.'",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "predeploy": "npm run build:prod",
    "predeploy:ssr:hosting": "npm run build:client-and-server-bundles && gulp firebase:ssr",
    "deploy": "firebase deploy --only hosting:normal",
    "deploy:functions": "firebase deploy --only functions",
    "deploy:ssr:hosting": "firebase deploy --only hosting:ssr",
    "deploy:ssr:functions": "firebase deploy --only functions:ssrApp",
    "docs": "compodoc -p tsconfig.json -d docs"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.0.0",
    "@angular/cdk": "^7.0.0",
    "@angular/common": "^7.0.0",
    "@angular/compiler": "^7.0.0",
    "@angular/core": "^7.0.0",
    "@angular/forms": "^7.0.0",
    "@angular/http": "^7.0.0",
    "@angular/material": "^7.0.0",
    "@angular/platform-browser": "^7.0.0",
    "@angular/platform-browser-dynamic": "^7.0.0",
    "@angular/platform-server": "^7.0.0",
    "@angular/router": "^7.0.0",
    "@angular/service-worker": "^7.0.0",
    "@nguniversal/express-engine": "^7.0.0",
    "@nguniversal/module-map-ngfactory-loader": "^7.0.0",
    "algoliasearch": "^3.26.0",
    "algoliasearch-helper": "^2.24.0",
    "api-ai-javascript": "^2.0.0-beta.21",
    "core-js": "^2.5.4",
    "express": "^4.16.4",
    "hammerjs": "^2.0.8",
    "rxjs": "^6.3.3",
    "zone.js": "^0.8.24"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.10.0",
    "@angular/cli": "^7.0.0",
    "@angular/compiler-cli": "^7.0.0",
    "@angular/language-service": "^7.0.0",
    "@compodoc/compodoc": "^1.1.2",
    "@types/algoliasearch": "^3.24.10",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "firebase-tools": "^4.0.2",
    "gulp": "^3.9.1",
    "gulp-rename": "^1.4.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~1.4.2",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.3.0",
    "rimraf": "^2.6.2",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "typescript": "~3.1.1"
  }
}
